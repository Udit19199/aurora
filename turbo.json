{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"generate": {
			"cache": false,
			"outputs": [
				"generated/**"
			]
		},
		"build": {
			"dependsOn": [
				"^build",
				"generate"
			],
			"outputs": [
				"dist/**",
				".next/**",
				"!.next/cache/**",
				"**/.tsbuildinfo"
			]
		},
		"dev": {
			"dependsOn": [
				"^generate"
			],
			"cache": false,
			"persistent": true
		},
		"clean": {
			"cache": false
		},
		"db:migrate:dev": {
			"cache": false,
			"persistent": true
		},
		"db:migrate:deploy": {
			"cache": false
		},
		"db:push": {
			"cache": false
		},
		"db:seed": {
			"cache": false
		},
		"lint": {
			"dependsOn": [
				"^build"
			]
		}
	}
}
