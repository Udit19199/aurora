{
	"name": "@aurora/database",
	"version": "1.0.0",
	"main": "dist/client.js",
	"types": "dist/client.d.ts",
	"exports": {
		".": {
			"import": "./dist/client.mjs",
			"require": "./dist/client.js",
			"types": "./dist/client.d.ts"
		}
	},
	"scripts": {
		"build": "tsc --build",
		"clean": "rm -rf dist generated",
		"db:migrate:deploy": "dotenv -e ../../.env -- prisma migrate deploy",
		"db:migrate:dev": "dotenv -e ../../.env -- prisma migrate dev",
		"db:migrate:reset": "dotenv -e ../../.env -- prisma migrate reset",
		"db:push": "dotenv -e ../../.env -- prisma db push",
		"db:seed": "dotenv -e ../../.env -- tsx src/seed.ts",
		"dev": "tsc --build --watch",
		"format": "prisma format",
		"generate": "prisma generate",
		"lint": "eslint . --max-warnings 0",
		"studio": "prisma studio"
	},
	"devDependencies": {
		"@aurora/eslint-config": "workspace:*",
		"@aurora/typescript-config": "workspace:*",
		"@prisma/client": "^5.22.0",
		"@types/node": "^20.14.10",
		"dotenv-cli": "^7.4.2",
		"eslint": "^8.57.0",
		"prisma": "^5.22.0",
		"tsx": "4.19.1",
		"typescript": "5.5.4"
	}
}
